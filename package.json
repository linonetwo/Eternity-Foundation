{
  "name": "eternity-foundation",
  "productName": "Eternity-Foundation",
  "version": "1.0.0",
  "description": "此游戏关乎在高维时空中进行时间旅行，避免时空悖论毁灭整个「永恒时空基金会」，保护历史不受时间刺客的破坏。你是永恒之人。",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "installType": "typesync",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Lin Onetwo",
    "email": "linonetwo012@gmail.com"
  },
  "license": "Apache License Version 2.0",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "my_new_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.tsx",
                  "preload": {
                    "js": "./src/preload.ts"
                  },
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-rpm": "6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "@electron-forge/plugin-webpack": "6.0.0-beta.53",
    "@types/csp-html-webpack-plugin": "3.0.0",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/eslint": "7.2.4",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/file-loader": "4.2.0",
    "@types/fs-extra": "9.0.1",
    "@types/history": "4.7.8",
    "@types/lodash": "4.14.161",
    "@types/prettier": "2.1.1",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/styled-components": "5.1.3",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "@zeit/webpack-asset-relocator-loader": "0.8.0",
    "csp-html-webpack-plugin": "4.0.0",
    "css-loader": "4.3.0",
    "electron": "10.1.3",
    "electron-devtools-installer": "3.1.1",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-html": "6.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-unicorn": "22.0.0",
    "file-loader": "6.1.0",
    "fork-ts-checker-webpack-plugin": "5.2.0",
    "node-loader": "1.0.1",
    "prettier": "2.1.2",
    "react-hot-loader": "4.13.0",
    "style-loader": "1.3.0",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "typesync": "0.7.0"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.31.0",
    "@javelin/ecs": "^0.19.2",
    "@pixi/unsafe-eval": "^5.3.3",
    "@rematch/core": "^2.0.0-next.5",
    "@rematch/immer": "^2.0.0-next.5",
    "@rematch/loading": "^2.0.0-next.5",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "beautiful-react-hooks": "^0.30.0",
    "connected-react-router": "^6.8.0",
    "electron-squirrel-startup": "^1.0.0",
    "fast-fuzzy": "^1.10.7",
    "fs-extra": "^9.0.1",
    "history": "^4.10.1",
    "i18next": "^19.7.0",
    "i18next-electron-fs-backend": "^1.3.4",
    "immer": "^7.0.8",
    "lodash": "^4.17.20",
    "pixi.js": "^5.3.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.2",
    "react-pixi-fiber": "^1.0.0-beta.4",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "secure-electron-store": "^1.2.0",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7"
  }
}
